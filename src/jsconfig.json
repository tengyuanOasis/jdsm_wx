{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"strict": true,
		"jsx": "preserve",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"allowSyntheticDefaultImports": true,
		"useDefineForClassFields": true,
		"resolveJsonModule": true,
		"noImplicitAny": false,
		"allowJs": true,
		"lib": [
			"esnext",
			"dom"
		],
		"types": [
			"@dcloudio/types"
		]
	}
}