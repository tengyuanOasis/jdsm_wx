{
	"tabBar": {
		"borderStyle": "white",
		"selectedColor": "#ea3323",
		"backgroundColor": "#fff",
		"textColor": "#000000",
		"list": [
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/images/tab/home.png",
				"selectedIconPath": "static/images/tab/home_active.png",
				"text": "首页"
			},
			{
				"pagePath": "pages/category/category",
				"iconPath": "static/images/tab/more.png",
				"selectedIconPath": "static/images/tab/more_active.png",
				"text": "产品列表"
			},
			{
				"pagePath": "pages/mine/user",
				"iconPath": "static/images/tab/user.png",
				"selectedIconPath": "static/images/tab/user_active.png",
				"text": "我的"
			}
		]
	},
	"networkTimeout": {
		"request": 20000,
		"downloadFile": 10000
	},
	"sitemapLocation": "sitemap.json",
	"useDynamicPlugins": true,
	"ignoreDevUnusedFiles": false,
	"ignoreUploadUnusedFiles": false,
	"easycom": {
		"autoscan": true,
		"custom": {
			"^avatar$": "@/components/Avatar/index",
			"^container$": "@/components/Container/index",
			"^popup$": "@/components/Popup/index",
			"^empty$": "@/components/Empty/index",
			"my-(.*)": "@/components/my-$1/my-$1.vue",
			"fui-(.*)": "firstui-uni/firstui/fui-$1/fui-$1.vue"
		}
	},
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarBackgroundColor": "#ff0008",
		"titleBarColor": "#ea3323",
		"requiredPrivateInfos": ["chooseAddress"]
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"transparentTitle": "always",
				"titlePenetrate": "YES",
				"mp-alipay": {
					"allowsBounceVertical": "NO",
					"titlePenetrate": "YES"
				}
			}
		},
		{
			"path": "pages/search/search",
			"style": {
				"navigationBarTitleText": "搜索"
			}
		},
		{
			"path": "pages/category/category",
			"style": {
				"navigationBarTitleText": "产品列表"
			}
		},
		{
			"path": "pages/mine/user",
